<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_marginTop="@dimen/common_dimen_10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/ll_1"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="1"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="ABC"
                android:visibility="invisible"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_2"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="2"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="ABC"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_3"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="3"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="DEF"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="@dimen/common_dimen_5dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/ll_4"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="4"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="GHI"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_5"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="5"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="GKL"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_6"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="6"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="MNO"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="@dimen/common_dimen_5dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/ll_7"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="7"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="PQRS"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_8"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="8"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="TUV"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_9"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="9"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:text="WXYZ"
                android:textSize="@dimen/text_size_10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="@dimen/common_dimen_5dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="*"
                android:paddingTop="@dimen/common_dimen_8dp"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/text_gray"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_0"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="0"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="36sp"
                android:textColor="@color/alert_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


        <LinearLayout
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:text="#"
                android:textColor="@color/text_gray"
                android:fontFamily="sans-serif-light"
                android:textStyle="normal"
                android:textSize="30sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:padding="@dimen/common_dimen_20dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/ll_Calling"
            android:gravity="center"
            android:background="@drawable/fab_green"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:src="@drawable/ic_call_24dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
